{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "Car",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "car_Image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Car_Features": {
      "type": "component",
      "repeatable": false,
      "component": "car-details.car-features"
    },
    "Body": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "price": {
      "type": "decimal"
    },
    "used_status": {
      "type": "enumeration",
      "enum": [
        "None",
        "New",
        "Used",
        "Parts"
      ],
      "default": "None",
      "required": true
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "cars"
    }
  }
}
